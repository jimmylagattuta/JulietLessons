    "class-variance-authority": "^0.7.1",
    "dotenv": "^16.5.0",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "jotai": "^2.6.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "mongoose": "^8.15.2",
    "path": "^0.12.7",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
